{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "HealthOptions": {
    "Enabled": true
  },
  "HealthEndpointsOptions": {
    "HealthEndpointEnabled": true,
    "PingEndpointEnabled": true,
    "Timeout": "0:0:10"
  },
  "AllowedHosts": "*",
  "OriginsEnabled": "https://localhost:6001",
  "CustomErrors": {
    "errors": [
      {
        "Type": "NotFound",
        "Message": "",
        "HttpStatuscode": 404
      },
      {
        "Type": "BadRequest",
        "Message": "One or more validation errors occurred.",
        "HttpStatuscode": 400
      },
      {
        "Type": "InternalServerError",
        "Message": "",
        "HttpStatuscode": 500
      }
    ]
  }
}
