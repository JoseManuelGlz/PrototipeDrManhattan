{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "HealthOptions": {
    "Enabled": true
  },
  "HealthEndpointsOptions": {
    "HealthEndpointEnabled": true,
    "PingEndpointEnabled": true,
    "Timeout": "0:0:10"
  },
  "AllowedHosts": "*",
  "OriginsEnabled": "https://localhost:6001",
  "CustomErrors": {
    "errors": [
      {
        "type": "NotFound",
        "message": "",
        "httpStatuscode": 404
      },
      {
        "type": "BadRequest",
        "message": "One or more validation errors occurred.",
        "httpStatuscode": 400
      },
      {
        "type": "InternalServerError",
        "message": "",
        "httpStatuscode": 500
      }
    ]
  }
}
